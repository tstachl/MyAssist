<?xml version="1.0" encoding="utf-8" ?>
<application xmlns="http://ns.adobe.com/air/application/2.7" minimumPatchLevel="0">
	<id>com.sselabs.dev.myassist</id>
    <versionNumber>1.0.0</versionNumber>
    <versionLabel>v1</versionLabel>
